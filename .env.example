# LinkedIn Growth Workflow - Environment Variables
# Copy this file to .env and fill in your values

# Required: LinkedIn API Credentials
LINKEDIN_ACCESS_TOKEN=your_linkedin_oauth_token_here
LINKEDIN_PERSON_URN=urn:li:person:your_id_here

# Required: Gemini AI
GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Additional Data Sources
NEWS_API_KEY=your_newsapi_key_here
TAVILY_API_KEY=your_tavily_api_key_here

# Optional: Force a specific vibe (leave empty for random)
# Options: The Contrarian, The Visionary, The Educator, The Analyst, The Narrator
FORCED_VIBE=

# Optional: Override log level
LOG_LEVEL=INFO
